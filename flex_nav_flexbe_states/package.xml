<package>
	<name>flex_nav_flexbe_states</name>
	<version>0.2.0</version>
	<description>
		 flex_nav_flexbe_states provides a collection of FlexBE states used during the SSP Research
	</description>
	<maintainer email="robotics@cnu.edu">David Conner</maintainer>
	<license>BSD</license>

	<url>http://ros.org/wiki/flexbe_states</url>
	<author>Justin Willis</author>

	<buildtool_depend>catkin</buildtool_depend>

	<run_depend>rospy</run_depend>
	<run_depend>geometry_msgs</run_depend>
	<run_depend>flexbe_msgs</run_depend>
	<run_depend>flexbe_core</run_depend>

	<build_depend>rostest</build_depend>

	<export>
	    <flexbe_states />
	</export>

</package>
